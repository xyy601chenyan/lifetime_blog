<div class="col-md-4 col-md-offset-4">
  <h2>编辑文章</h2>

  <hr>
  <%= form_for @article,url: article_path(@article.token) do |f| %>
    标题
    <%= f.text_field :title %>
    <br>

    正文
    <br>
    <%= f.text_area :description %>
    <br>

    <%= f.submit "确定",disable_with: "提交中..."%>
  <% end %>
</div>
